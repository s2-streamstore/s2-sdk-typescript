{
  "name": "@s2-dev/streamstore-patterns",
  "version": "0.0.0",
  "type": "module",
  "license": "Apache-2.0",
  "homepage": "https://s2.dev",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "rm -rf dist && bun ../../scripts/generate-version.ts && tsc --project tsconfig.build.esm.json && tsc --project tsconfig.build.cjs.json && bun ../../scripts/postbuild.ts",
    "test": "vitest --run -c ../../vitest.config.ts",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@s2-dev/streamstore": "^0.0.0"
  }
}

